{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true,
    "moduleSuffixes": [".native", ""],
    "paths": {
      "@/*": [
        "./src/app/*", 
        "./src/modules/shared/*", 
        "./src/modules/dashboard/*", 
        "./src/modules/router/*", 
        "./src/modules/auth/*"
      ]
    }
  },
  "include": [
    "./declarations.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ]
}
